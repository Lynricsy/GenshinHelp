#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/daye99/Desktop/demo/mihayo/node_modules/.pnpm/sshpk@1.16.1/node_modules/sshpk/bin/node_modules:/Users/daye99/Desktop/demo/mihayo/node_modules/.pnpm/sshpk@1.16.1/node_modules/sshpk/node_modules:/Users/daye99/Desktop/demo/mihayo/node_modules/.pnpm/sshpk@1.16.1/node_modules:/Users/daye99/Desktop/demo/mihayo/node_modules/.pnpm/node_modules:/Users/daye99/Desktop/demo/mihayo/node_modules:/Users/daye99/Desktop/demo/node_modules:/Users/daye99/Desktop/node_modules:/Users/daye99/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/daye99/Desktop/demo/mihayo/node_modules/.pnpm/sshpk@1.16.1/node_modules/sshpk/bin/node_modules:/Users/daye99/Desktop/demo/mihayo/node_modules/.pnpm/sshpk@1.16.1/node_modules/sshpk/node_modules:/Users/daye99/Desktop/demo/mihayo/node_modules/.pnpm/sshpk@1.16.1/node_modules:/Users/daye99/Desktop/demo/mihayo/node_modules/.pnpm/node_modules:/Users/daye99/Desktop/demo/mihayo/node_modules:/Users/daye99/Desktop/demo/node_modules:/Users/daye99/Desktop/node_modules:/Users/daye99/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/sshpk-verify" "$@"
else
  exec node  "$basedir/../../bin/sshpk-verify" "$@"
fi
